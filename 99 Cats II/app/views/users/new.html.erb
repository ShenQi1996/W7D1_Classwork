<h1>Create a New User</h1>

<form action="<%= users_url %>" method="POST">    
    
    <label>Username
        <input type="text" name="user[user_name]" value="<%= @user.user_name %>">
    </label>

    <label>Password
        <input type="password" name="user[password]" value="">
    </label>

    <button>Submit</button>

</form>